{
  "expo": {
    "name": "todo list",
    "slug": "todo-list",
    "version": "1.0.0",
    "orientation": "default",
    "icon": "./assets/icon.png",
    "splash": {
      "image": "./assets/todo.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/todo.png",
        "backgroundColor": "#FFFFFF"
      },
      "package": "myfirstProg.apk"
    },
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "description": "Almost ready mini-project for my portfolio ",
    "githubUrl": "https://github.com/tisukRoman/todoList"
  }
}
